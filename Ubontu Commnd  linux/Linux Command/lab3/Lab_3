echo "Enter 1st Number:"
read num1

echo "Enter 2nd Number"
read num2

if [ "$num1" -gt "$num2" ]
  then
     echo "The large  number is $num1 & small number is $num2"


elif [ "$num2" -gt "$num1" ]
  then
     echo "The large  number is $num2 & small number is $num1"





elif [ "$num1" -eq "$num2" ]
  then 
     echo "Number1 & Number2 same "





else 
   echo "Invalid User Number"



fi
